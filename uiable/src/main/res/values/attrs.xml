<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="enum" name="draggable_style">
        <enum name="collision" value="0" />
        <enum name="stickyX" value="1" />
        <enum name="stickyY" value="2" />
        <enum name="stickyXY" value="3"/>
    </attr>
    <attr format="boolean" name="clickable_support"/>

    <declare-styleable name="Draggable">
        <attr name="draggable_style"/>
        <attr name="clickable_support"/>
    </declare-styleable>
</resources>